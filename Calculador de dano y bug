def verificar_nivel_dano(dano):
    if dano > 10:
        print("Dano critico")
    else:
        print("Dano baixo")

verificar_nivel_dano(35) # Dano critico
verificar_nivel_dano(10) # Dano baixo
verificar_nivel_dano(5) # Dano baixo

dano_usuario = int(input("Quanto dano voce fez?"))
verificar_nivel_dano(dano_usuario)
