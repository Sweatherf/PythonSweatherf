numero = int(input("Digite a sua nota: "))

match numero:
    case 1 | 2 | 3:
        print(f"Sua nota foi {numero} Voce está reprovado!!!")
    case 4 | 5 | 6:
        print(f"Sua nota foi {numero} Voce pode fazer recuperaçao!!!")
    case 7 | 8 | 9 | 10:
        print(f"Sua nota foi {numero} Voce está de parabens!!!")
